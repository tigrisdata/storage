{
  "name": "@tigrisdata/react",
  "version": "0.0.1",
  "description": "React components and hooks for Tigris object storage",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "publint": "publint",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx}",
    "prepublishOnly": "npm run build",
    "test": "vitest run --config vitest.config.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "react",
    "object",
    "storage",
    "tigris",
    "blob",
    "ai",
    "upload",
    "components",
    "hooks"
  ],
  "author": "Tigris Data",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tigrisdata/storage.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@tigrisdata/storage": "*"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
