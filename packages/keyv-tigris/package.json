{
  "name": "@tigrisdata/keyv-tigris",
  "version": "0.0.1",
  "description": "Keyv storage adapter for Tigris",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "publint": "publint"
  },
  "keywords": [
    "keyv",
    "adapter",
    "tigris",
    "storage"
  ],
  "author": "Tigris Data",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tigrisdata/storage.git",
    "directory": "packages/keyv-tigris"
  },
  "license": "MIT",
  "dependencies": {
    "@tigrisdata/storage": "*",
    "dotenv": "^17.2.1",
    "keyv": "^5.5.4"
  },
  "devDependencies": {
    "@keyv/test-suite": "^2.1.2"
  }
}
