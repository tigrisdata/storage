{
  "name": "@tigrisdata/iam",
  "version": "0.0.1",
  "description": "Tigris SDK for IAM operations",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "publint": "publint",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "prepublishOnly": "npm run build",
    "test": "vitest run --config vitest.config.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "iam",
    "organization",
    "tigris",
    "storage"
  ],
  "author": "Tigris Data",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tigrisdata/storage.git",
    "directory": "packages/iam"
  },
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.1"
  }
}
